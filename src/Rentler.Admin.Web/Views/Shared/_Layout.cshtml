@{ 
    var angular = "@angular";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Rentler Admin - @RenderSection("title",false)</title>
    <base href="~/" />
    <!--<link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />-->
    <environment include="Development">
        <link href="~/Content/metro-bootstrap.css" rel="stylesheet" media="screen" asp-append-version="true" />
        <link href="~/Content/font-awesome.css" rel="stylesheet" media="screen" asp-append-version="true" />
        <link href="~/Content/wysihtml5/bootstrap-wysihtml5.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Css/rentlerawesome.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Css/crocodoc.viewer.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Css/admin.css" rel="stylesheet" media="screen" asp-append-version="true" />
        <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon" asp-append-version="true" />
        <link rel="stylesheet" href="~/libs/@angular/material/prebuilt-themes/indigo-pink.css" asp-append-version="true" />
        <link rel="stylesheet" href="www.google.com?v=@Configuration.App.AzureQueueNamePrefix" />
    </environment>
    <environment exclude="Development">
    <link href="~/rentler.style.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    
  
    @RenderSection("css", false)
</head>
<body>
    @RenderBody()

    <environment exclude="development">
        <script type="text/javascript" src="~/scripts/jquery-2.1.0.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/jquery.validate.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/jquery-ui-1.10.4.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/bootstrap.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/Content/wysihtml5/wysihtml5-0.3.0.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/Content/wysihtml5/bootstrap3-wysihtml5.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/ui-bootstrap-0.10.0.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/ui-bootstrap-tpls-0.10.0.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/d3.v3.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/crocodoc.viewer.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/masks.js" asp-append-version="true"></script>
        <script type="text/javascript" src="~/scripts/moment.min.js" asp-append-version="true"></script>
    </environment>
    <environment include="development">
        <script type="text/javascript" src="~/rentler.script.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
